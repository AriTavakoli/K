    "date_index = []\n",
    "for i in parsed:\n",
    "    if re.match('[0-9/-]',i):\n",
    "        date_index.append(parsed.index(i))\n",
    "main_experience = []        \n",
    "for i,t in zip(techy,date_index):\n",
    "    main_experience.append(parsed[t-1:i+1])\n"
