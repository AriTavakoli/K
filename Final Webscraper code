    "# main webscraper that takes all the information from a user's profile \n",
    "name = []\n",
    "short_description = []\n",
    "bio = []\n",
    "availability = []\n",
    "preferred_environment = [] \n",
    "portfolio = []\n",
    "experience = [] \n",
    "employment = []\n",
    "main_experience = []\n",
    "date_index = []\n",
    "techy = []\n",
    "languages = [] \n",
    "Libraries_APIs = []\n",
    "tools = [] \n",
    "paradigms = []\n",
    "platforms = []\n",
    "other = []\n",
    "frameworks = []\n",
    "storage = [] \n",
    "Education = []\n",
    "\n",
    "Experience2 = [] \n",
    "\n",
    "\n",
    "def Scraper(link):\n",
    "    driver.get(link)\n",
    "    \n",
    "    \n",
    "    \n",
    "    # Name \n",
    "    try:\n",
    "        \n",
    "        name.append(driver.find_element_by_class_name('resume_top-info_name').text)\n",
    "    except NoSuchElementException:\n",
    "        \n",
    "        name.append('Nan')\n",
    "    #Short description\n",
    "    try:\n",
    "        short_description.append(driver.find_element_by_class_name('resume_top-info_short_description').text)\n",
    "    except NoSuchElementException:\n",
    "        short_description.append('Nan')\n",
    "    # Bio\n",
    "    try:\n",
    "        bio.append(driver.find_element_by_class_name('resume_top-info_bio').text)\n",
    "    except NoSuchElementException:\n",
    "        bio.append('Nan')\n",
    "    # Portfolio \n",
    "    try:\n",
    "        portfolio.append(driver.find_element_by_xpath('//*[@id=\"Portfolio\"]/div/div/div[1]/div/div/ul').text)\n",
    "    except NoSuchElementException:\n",
    "        portfolio.append('Nan')\n",
    "\n",
    "    #Employment \n",
    "    try:\n",
    "        employment.append(driver.find_element_by_xpath('//*[@id=\"Employment\"]/div/div/div/div').text)\n",
    "    except NoSuchElementException:\n",
    "        employment.append('Nan')\n",
    "    try:\n",
    "    # Expereince\n",
    "        experience.append(driver.find_element_by_xpath('//*[@id=\"Portfolio\"]/div/div/div[2]/div/div/ul').text)\n",
    "    except:\n",
    "        experience.append('Nan')\n",
    "    \n",
    "    #experience 2\n",
    "    try:\n",
    "        Experience2.append(driver.find_element_by_xpath('//*[@id=\"Experience\"]/div/div/div/div').text)\n",
    "    except NoSuchElementException:\n",
    "        Experience2.append('Nan')\n",
    "    # Availability \n",
    "    try:\n",
    "        availability.append(driver.find_element_by_xpath('//*[@id=\"Availability\"]/div/div/div[1]/div/div/div').text)\n",
    "    except NoSuchElementException:\n",
    "        availability.append('Nan')\n",
    "    try:\n",
    "    # Preferred_envioronment \n",
    "        preferred_environment.append(driver.find_element_by_xpath('//*[@id=\"Availability\"]/div/div/div[2]/div/div/div').text)\n",
    "    except NoSuchElementException:\n",
    "        preferred_environment.append('Nan')\n",
    "        \n",
